[
  {
    "Resource": "*",
    "Action": [
      "application-autoscaling:*",
      "cloudformation:*",
      "cloudwatch:*",
      "ec2:*",
      "events:*",
      "iam:*",
      "kms:*",
      "kinesis:*",
      "lambda:*",
      "logs:*",
      "rds:*",
      "rds-data:*",
      "rds-db:*",
      "s3:*",
      "secretsmanager:*",
      "sns:*",
      "ssm:*",
      "ssmmessages:*",
      "sts:*",
      "pi:*"
    ],
    "Effect":"Allow",
    "Sid":"PermissivePolicy"
  },
  {
     "Resource":"$$teamRoleArn",
     "Action":"iam:PassRole",
     "Effect":"Allow",
     "Sid":"PassRole"
  }
]
